    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rootLayout"
        >
        <RelativeLayout
            android:id="@+id/top_bar_dashboard"
            android:background="@color/colorAccent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        </RelativeLayout>

        <LinearLayout
            android:id="@+id/total_header"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorAccent"
            >

            <RelativeLayout
                android:id="@+id/total_header_left"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15sdp"
                android:background="@color/colorWhite"
                >

                <ImageView
                    android:id="@+id/left_header"
                    android:layout_width="@dimen/_35sdp"
                    android:layout_height="@dimen/_35sdp"
                    android:layout_centerInParent="true"


                    />

                <com.app.l_pesa.common.CommonTextRegular
                    android:layout_below="@+id/left_header"
                    android:id="@+id/left_header_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorApp"
                    android:textSize="@dimen/_12sdp"
                    android:layout_centerHorizontal="true"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="@dimen/_1sdp"
                android:layout_height="match_parent"
                android:paddingTop="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:background="@color/colorAccent"

                >

                <LinearLayout
                    android:layout_width="@dimen/_1sdp"
                    android:layout_height="match_parent"
                    android:background="@color/colorAccent"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:layout_marginBottom="@dimen/_4sdp"
                    android:layout_centerInParent="true">

                </LinearLayout>


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/total_header_right"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorWhite"
                android:gravity="center"
                android:layout_marginEnd="@dimen/_15sdp"
                >

                <ImageView
                    android:id="@+id/right_header"
                    android:layout_width="@dimen/_35sdp"
                    android:layout_height="@dimen/_35sdp"
                    android:layout_centerInParent="true"

                    />

                <com.app.l_pesa.common.CommonTextRegular
                    android:layout_below="@+id/right_header"
                    android:id="@+id/right_header_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorApp"
                    android:textSize="@dimen/_12sdp"
                    android:layout_centerHorizontal="true"
                    />

            </RelativeLayout>

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/header_below_layout"
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_below="@+id/total_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingEnd="@dimen/_5sdp"
            android:paddingStart="@dimen/_5sdp"
            >
            <TextView
                android:id="@+id/credit_score_lable"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your L-Pesa Credit Score"

                android:layout_marginTop="@dimen/_7sdp"
                />

            <SeekBar
                android:id="@+id/seek_bar"
                android:layout_centerInParent="true"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp"


                />
            <ImageView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"/>

            <com.app.l_pesa.common.CommonTextRegular
                android:id="@+id/full_score"
                android:layout_alignParentEnd="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/seek_bar"
                android:layout_marginTop="@dimen/_3sdp"

                />
            <com.app.l_pesa.common.CommonTextRegular
                android:id="@+id/txt_score"
                android:layout_alignParentStart="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/seek_bar"
                android:layout_marginTop="6dp"

                />
            <com.app.l_pesa.common.CommonTextRegular
                android:id="@+id/txt_made"
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/seek_bar"
                android:layout_marginTop="6dp"

                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/banner_layout"
            android:layout_below="@+id/header_below_layout"
            android:layout_height="@dimen/_50sdp"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginEnd="@dimen/_15sdp">
            <ImageView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                />
            <ImageView
                android:id="@+id/close_banner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_margin="@dimen/_5sdp"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/banner_layout"
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_15sdp">

            <android.support.v7.widget.RecyclerView
                android:id="@+id/loan_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                >

            </android.support.v7.widget.RecyclerView>

        </RelativeLayout>

    </RelativeLayout>

